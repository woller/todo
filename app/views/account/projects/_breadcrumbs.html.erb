<% project ||= @project %>
<% team ||= @team || project&.team %>
<%= render 'account/teams/breadcrumbs', team: team %>
<%= render 'account/shared/breadcrumb', label: t('.label'), url: [:account, team, :projects] %>
<% if project&.persisted? %>
  <%= render 'account/shared/breadcrumb', label: project.label_string, url: [:account, project] %>
<% end %>
<%= render 'account/shared/breadcrumbs/actions', only_for: 'projects' %>
